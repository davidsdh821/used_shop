<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.usedshop.images.dao.ImagesDAO">
	<insert id="insertImages">
		INSERT INTO
			`images`
			(
			`postId`
			,`imagePath`
			,`createdAt`
			,`updatedAt`
			)
			VALUES
			(
			#{postId}
			,#{imagePath}
			,NOW()
			,NOW()
			)			
	
	
	
	</insert>


</mapper>